@model quotes_project.Models.ListadoModel
@{
    ViewData["Title"] = "Listado";
}

<h1>Cotizaciones</h1>

<div class="page-container">
    <div class="table-container">
        @if (Model.HtmlTable != null)
        {
            @Html.Raw(Model.HtmlTable)
        }
        else
        {
            <p>No se pudo generar la tabla HTML.</p>
        }
    </div>
</div>
