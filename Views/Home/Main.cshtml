@{
    ViewData["Title"] = "Main";
}

<div class="text-center">
    <h2 class="display-4" style="border-top-width: 0px;border-top-style: solid;margin-top: 40px;margin-bottom: 40px;">Bienvenido</h2>
</div>
<div class="user-dashboard">
    <div class="user-info">
        <style type="text/css">
            .tg {
                border-collapse: collapse;
                border-spacing: 0;
                margin: 0px auto;
            }

                .tg td {
                    border-color: black;
                    border-style: solid;
                    border-width: 1px;
                    font-family: Arial, sans-serif;
                    font-size: 14px;
                    overflow: hidden;
                    padding: 10px 5px;
                    word-break: normal;
                }

                .tg th {
                    border-color: black;
                    border-style: solid;
                    border-width: 1px;
                    font-family: Arial, sans-serif;
                    font-size: 14px;
                    font-weight: normal;
                    overflow: hidden;
                    padding: 10px 5px;
                    word-break: normal;
                }

                .tg .tg-baqh {
                    text-align: center;
                    vertical-align: top
                }
        </style>
        <table class="tg">
            <thead>
                <tr>
                    <th class="tg-baqh" colspan="2">Tus cotizaciones de este mes</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td class="tg-baqh">Cotizaciones enviadas</td>
                    <td class="tg-baqh">Cotizaciones pendientes de seguimiento</td>
                </tr>
            </tbody>
        </table>
    </div>

</div>
